package com.aj.frame.ydjwpt.common.beans;

import com.aj.frame.um.usm.beans.User;

/** 
 * @ClassName: UserOperateBehavior 
 * @Description: 用户操作行为分析
 * @author liuling 
 * @date 2018-8-3 上午09:52:04 
 * 
 */
public class UserOperateBehavior {
	
	/**
	 * 用户对象，服务端处理器可以通过会话获取请求用户对象信息；特殊情况可有请求方传入
	 */
	private User user;
	
	/**
	 * 操作行为标识：1-APP下载；2-App访问；3-App热搜；
	 */
	private int optBehavior;
	
	/**
	 * 操作行为描述
	 */
	private String behaviorDesc;
	
	/**
	 * 涉及的操作对象主键
	 */
	private String objId;
	
	/**
	 * 涉及操作对象的Json格式描述信息
	 */
	private String objJson;

	public User getUser() {
		return user;
	}

	public void setUser(User user) {
		this.user = user;
	}

	public int getOptBehavior() {
		return optBehavior;
	}

	public void setOptBehavior(int optBehavior) {
		this.optBehavior = optBehavior;
	}

	public String getBehaviorDesc() {
		return behaviorDesc;
	}

	public void setBehaviorDesc(String behaviorDesc) {
		this.behaviorDesc = behaviorDesc;
	}

	public String getObjId() {
		return objId;
	}

	public void setObjId(String objId) {
		this.objId = objId;
	}

	public String getObjJson() {
		return objJson;
	}

	public void setObjJson(String objJson) {
		this.objJson = objJson;
	}
	
}
